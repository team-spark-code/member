# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Google OAuth2 Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Kakao OAuth2 Configuration
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret

# Database Configuration (optional - defaults are in application.properties)
DB_URL=jdbc:mariadb://localhost:3306/redfin?useUnicode=true&characterEncoding=utf8mb4&serverTimezone=Asia/Seoul
DB_USERNAME=root
DB_PASSWORD=your-database-password

# Elasticsearch Configuration (optional)
ELASTICSEARCH_URL=http://localhost:9200